{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "max-len": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "warn"
  }
}
